local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Magic Clicker By VernyHub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddToggle({
	Name = "Auto Click",
	Default = false,
	Callback = function(Value)
		_G.autoc = Value;
	end    
})


task.spawn(function()
    while wait() do
        if _G.autoc then
            game:GetService("ReplicatedStorage").Remotes.CheckClick:FireServer()
            wait()
        end
    end
end)

local Section = Tab:AddSection({
	Name = "Rebirth"
})


local rebirths = {}

for i=1,46 do
    table.insert(rebirths,i)
end

Tab:AddDropdown({
	Name = "Select Rebirth Option",
	Default = " ",
	Options = rebirths,
	Callback = function(Value)
		_G.rebop = Value;
	end    
})

Tab:AddToggle({
	Name = "Auto Rebirth",
	Default = false,
	Callback = function(Value)
		_G.autoreb = Value;
	end    
})

task.spawn(function()
    while wait() do
        if _G.autoreb then
            if _G.rebop ~= nil then
                game:GetService("ReplicatedStorage").Remotes.BuyRebirth:FireServer(_G.rebop)
                wait(0.2)
            end
        end
    end
end)

local Tab = Window:MakeTab({
	Name = "Auto Eggs",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Select World"
})

local eggs = game:GetService("Workspace").Eggs

local worlds = {}

for i,v in ipairs(eggs:GetChildren()) do
    if v:IsA("Folder") then
        table.insert(worlds, v.Name)
    end
end

local eggsV2 = {}

for i=1,30 do
    table.insert(eggsV2,i)
end



Tab:AddDropdown({
	Name = "Select World",
	Default = " ",
	Options = worlds,
	Callback = function(Value)
		_G.worlds = Value;
	end    
})

local Section = Tab:AddSection({
	Name = "Select Eggs"
})

Tab:AddDropdown({
	Name = "Select Egg",
	Default = " ",
	Options = eggsV2,
	Callback = function(Value)
		_G.eggsv2 = Value;
	end    
})


Tab:AddDropdown({
	Name = "Select Egg Open Amount",
	Default = " ",
	Options = {1,3},
	Callback = function(Value)
		_G.amount = Value;
	end    
})


Tab:AddToggle({
	Name = "Auto Open Egg(Must Be Near)",
	Default = false,
	Callback = function(Value)
		_G.autoegg = Value;
	end    
})

task.spawn(function()
    while wait() do
        if _G.autoegg then
            if _G.eggsv2 ~= nil then
                if _G.worlds ~= nil then
                    if _G.amount ~= nil then
                        game:GetService("ReplicatedStorage").Remotes.BuyEgg:FireServer(_G.eggsv2,_G.amount,workspace.Eggs[_G.worlds][_G.eggsv2])
                        wait(0.5)
                    end  
                end
            end
        end
    end
end)

local Section = Tab:AddSection({
	Name = "Pets & Stuff"
})

Tab:AddSlider({
	Name = "Time To Wait",
	Min = 1,
	Max = 120,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Seconds",
	Callback = function(Value)
		_G.time = Value;
	end    
})

Tab:AddToggle({
	Name = "Auto Craft All Pets",
	Default = false,
	Callback = function(Value)
		_G.autocraft = Value;
	end    
})

task.spawn(function()
    while wait() do
        if _G.autocraft then
            game:GetService("ReplicatedStorage").Remotes.CraftAllPets:FireServer(true)
            wait(_G.time)
        end
    end
end)

Tab:AddToggle({
	Name = "Auto Equip Best Pets",
	Default = false,
	Callback = function(Value)
		_G.autobest = Value;
	end    
})

task.spawn(function()
    while wait() do
        if _G.autobest then
            game:GetService("ReplicatedStorage").Remotes.EquipBest:FireServer()
            wait(_G.time)
        end
    end
end)

local Tab = Window:MakeTab({
	Name = "Teleports",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local worldsYes = game:GetService("Workspace").Worlds


local worldsName = {}

for i,v in ipairs(worldsYes:GetChildren()) do
    if v:IsA("Folder") then
        table.insert(worldsName, v.Name)
    end
end


Tab:AddDropdown({
	Name = "Select World",
	Default = " ",
	Options = worldsName,
	Callback = function(Value)
		_G.tpworld = Value;
	end    
})

local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart

Tab:AddButton({
	Name = "Teleport To Selected World",
	Callback = function()
        hrp.CFrame = game:GetService("Workspace").Worlds[_G.tpworld].Teleport.tp.CFrame
  	end    
})

local Tab = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddSlider({
	Name = "Change WalkSpeed",
	Min = 50,
	Max = 500,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		_G.WS = Value;
	end    
})


Tab:AddToggle({
	Name = "Toggle WalkSpeed",
	Default = false,
	Callback = function(Value)
		_G.togWS = Value;
	end    
})

task.spawn(function()
    while wait() do
        if _G.togWS then
            if _G.WS ~= nil then
                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = _G.WS
            end
        end
    end
end)

local Tab = Window:MakeTab({
	Name = "Credit",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddParagraph("Credits Below:","!IsagiYoichi#7765")

Tab:AddParagraph("Join My Discord","https://discord.gg/Vt6WpNb5")

Tab:AddButton({
	Name = "Copy Discord Link",
	Callback = function()
        setclipboard("https://discord.gg/Vt6WpNb5")
  	end    
})

